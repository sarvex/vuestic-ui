  "homepage": "https://vuestic.dev",
  "repository": "https://github.com/epicmaxco/vuestic-ui",
  "main": "dist/cjs/main.js",
  "module": "dist/es/main.js",
  "jsdelivr": "dist/iife/main.js",
  "unpkg": "dist/iife/main.js",
